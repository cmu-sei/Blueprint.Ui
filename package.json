{
  "name": "blueprint-ui",
  "version": "1.5.0",
  "license": "MIT",
  "config": {
    "openapiArgs": "--additional-properties ngVersion=15.1 --additional-properties modelPropertyNaming=original --type-mappings=DateTime=Date",
    "deleteGeneratedCommand": "rimraf src/app/generated/blueprint.api/"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint -c .eslintrc.js --ext .ts src/",
    "e2e": "ng e2e",
    "swagger:gen": "rimraf src/app/generated/blueprint.api/* && curl --insecure http://localhost:4724/swagger/v1/swagger.json --output swagger.json && @openapitools/openapi-generator-cli generate -i ./swagger.json -g typescript-angular -o src/app/generated/blueprint.api %npm_package_config_openapiArgs%",
    "swagger:gen-docker": "cross-var $npm_package_config_deleteGeneratedCommand && cross-env-shell docker run --rm -v $INIT_CWD:/local openapitools/openapi-generator-cli:v6.2.0 generate -i http://host.docker.internal:4724/swagger/v1/swagger.json?format=openapi -g typescript-angular -o /local/src/app/generated/blueprint.api $npm_package_config_openapiArgs",
    "docker:init": "docker network create identity --driver=overlay",
    "docker:stack": "npm run docker:stack:identity && npm run docker:stack:api",
    "docker:stack:api": "docker stack deploy -c docker-compose.yml -c docker/compose/api.yml blueprint_stack",
    "docker:stack:identity": "docker stack deploy -c docker-compose.yml -c docker/compose/identity.yml identity_stack",
    "docker:destroy": "docker stack rm blueprint_stack identity_stack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.13",
    "@angular/cdk": "^20.2.13",
    "@angular/common": "^20.3.13",
    "@angular/compiler": "^20.3.13",
    "@angular/core": "^20.3.13",
    "@angular/forms": "^20.3.13",
    "@angular/material": "^20.2.13",
    "@angular/platform-browser": "^20.3.13",
    "@angular/platform-browser-dynamic": "^20.3.13",
    "@angular/router": "^20.3.13",
    "@cmusei/crucible-common": "0.3.0",
    "@datorama/akita": "^8.0.0",
    "@datorama/akita-ng-router-store": "^7.0.0",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@kolkov/angular-editor": "^3.0.0-beta.0",
    "@mdi/font": "^7.4.47",
    "@microsoft/signalr": "^8.0.7",
    "ajv": "^8.6.0",
    "core-js": "^3.14.0",
    "deep-copy-ts": "^0.5.4",
    "eventsource": "^1.1.1",
    "material-design-icons": "^3.0.1",
    "ngx-clipboard": "^15.1.0",
    "oidc-client": "^1.11.5",
    "rxjs": "^7.8.1",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.4.1",
    "uuid": "^8.3.2",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.11",
    "@angular-eslint/builder": "20.6.0",
    "@angular-eslint/eslint-plugin": "20.6.0",
    "@angular-eslint/eslint-plugin-template": "20.6.0",
    "@angular-eslint/schematics": "20.6.0",
    "@angular-eslint/template-parser": "20.6.0",
    "@angular/cli": "^20.3.11",
    "@angular/compiler-cli": "^20.3.13",
    "@angular/language-service": "^20.3.13",
    "@openapitools/openapi-generator-cli": "1.0.18-3.2.0",
    "@playwright/test": "^1.40.1",
    "@types/jasmine": "~3.7.7",
    "@types/jasminewd2": "~2.0.9",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.50.0",
    "@typescript-eslint/parser": "^7.2.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.7.5",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "~10.0.0",
    "typescript": "~5.9.3"
  }
}
